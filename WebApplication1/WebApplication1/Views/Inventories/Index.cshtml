@model IEnumerable<WebApplication1.Models.Inventory>

@{
    ViewBag.Title = "Index";
}

<h2>在庫管理</h2>

<p>
    @Html.ActionLink("商品管理画面へ移動", "Index","Products")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.商品番号)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.商品名)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.価格)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.個数)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.小計)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.最終更新)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>

    <td>
        @Html.DisplayFor(modelItem => item.商品番号)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.商品名)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.価格)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.個数)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.小計)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.最終更新)
    </td>
    <td>
        @Html.ActionLink("在庫数入力", "Edit", new { id = item.商品番号 }) |
        @Html.ActionLink("詳細", "Details", new { id = item.商品番号 })
    </td>
</tr>
}

</table>
